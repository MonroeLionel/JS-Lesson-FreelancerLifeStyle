/*в JS есть несколько типов значений (value)
фиксированые значения (литералы) значения констант
и значения переменных */
//привемы литералов
25                //литерал целого числа
23.8              //литерал дробного числа  
"джава скрипт"    //литерал строки
'джава скрипт'    //литерал строки
[]                //литерал масива
[15, 20, 99]        //литерал массива
{ }                //литерал объекта
{
  name: "vasan"
  surname: "prou"
}                 //литерал объекта
(ab | ba)           //литерал регулярного выражения
//в языках пограмирования константы и переменные
// используются для храннения значения данных
const MAX_VALUE = 17;           //константа
var section = "js";            //переменная
let arr = ['html', 'css', 'js']   //переменная